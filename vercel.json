{
  "routes": [
    {
      "src": "/dex/(.*)",
      "dest": "http://182.44.1.58:4181/dex/$1"
    },
    {
      "src": "/favicon/(.*)",
      "dest": "http://182.44.1.58:4181/favicon/$1",
      "continueOnTimeout": true
    },
    {
      "src": "/(.*)",
      "dest": "http://182.44.1.58:4181/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
